<h1 class="text-center my-3">All cats</h1>

<div class="container">
  <div class="row">
    <% @cats.each do |cat| %>
      <div class="col-11 col-md-4 col-lg-3">
        <div class="card mb-4 shadow">
          <img src="<%= cat.picture %>" alt="" class="card-img-top" height="250" >
          <div class="card-body">
            <h5 class="card-title"><%= cat.name %></h5>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
